---
interface Props {
  width?: number;
  height?: number;

  class?: string;
  style?: string;
}
---

<svg
  class={`${Astro.props.class ?? ''}`}
  style={`${Astro.props.style ?? ''}`}
  width={Astro.props.width ?? 64}
  height={Astro.props.height ?? 64}
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  stroke-width="2"
  stroke="currentColor"
  fill="none"
  stroke-linecap="round"
  stroke-linejoin="round"
  ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
    d="M12 12m-4 0a4 4 0 1 0 8 0a4 4 0 1 0 -8 0"></path><path
    d="M16 12v1.5a2.5 2.5 0 0 0 5 0v-1.5a9 9 0 1 0 -5.5 8.28"></path></svg
>
